# Disguise as MS Edge on Windows 10
# See also .wgetrc user_agent
user-agent = "Mozilla/5.0 (Windows NT 10.0; Win64; x64; WebView/3.0) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/  58.0.3029.110 Safari/537.36 Edge/16.16299"

# When following a redirect, automatically set the previous URL as referer
referer = ";auto"

# Wait 60 seconds before timing out
connect-timeout = 60

# https://github.com/drduh/macOS-Security-and-Privacy-Guide#curl
progress-bar
max-time = 300
#verbose
show-error
#remote-time
ipv4
